<div class="wrapper">

  <h1 class="heading">task一覧</h1>
  <%= form_with url:  "/tasks", method: :post, id: "form" do |form| %>
    <%= form.text_field :title, id: "title" %>
    <%= form.submit '投稿する' , id: "submit" %>
  <% end %>

  <div id="list">
  </div>

  <% @tasks.each do |task| %>
  <div class="task">
    <div class="task-date">
      投稿日時：<%= task.created_at %>
    </div>
    <div class="task-title">
      <%= task.title %>
    </div>
  </div>
  <% end %>

</div>